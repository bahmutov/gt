{
  "name": "gt",
  "description": "JavaScript native QUnit runner with code coverage and multiple extensions",
  "version": "0.9.0",
  "author": {
    "name": "Gleb Bahmutov",
    "email": "gleb.bahmutov@gmail.com",
    "website": "http://glebbahmutov.com"
  },
  "_from": "gt@",
  "_id": "gt@0.8.5",
  "bin": {
    "gt": "./bin/gt.js"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/gt/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "grunt",
        "npm run mocha",
        "npm run unit",
        "npm run e2e",
        "npm run bdd"
      ],
      "pre-push": [
        "npm install",
        "grunt",
        "npm test"
      ],
      "post-commit": "npm version",
      "post-merge": []
    }
  },
  "contributors": [],
  "czConfig": {
    "path": "node_modules/pre-git/node_modules/cz-conventional-changelog"
  },
  "dependencies": {
    "allong.es": "0.14.0",
    "amdefine": "0.1.1",
    "async": "1.3.0",
    "check-types": "1.4.0",
    "cli-color": "1.0.0",
    "coffee-script": "1.6.3",
    "console.json": "0.2.1",
    "custom-logger": "0.3.1",
    "glob": "5.0.13",
    "istanbul": "0.3.17",
    "lazy-ass": "0.5.8",
    "lodash": "3.10.0",
    "nodewatch": "0.3.2",
    "optimist": "0.6.1",
    "optional-color-logger": "0.0.6",
    "q": "1.4.1",
    "quote": "0.4.0",
    "sprintf": "0.1.5",
    "untested": "0.1.5",
    "update-notifier": "0.5.0"
  },
  "devDependencies": {
    "assert": "1.3.0",
    "bad-line": "0.1.1",
    "coveralls": "2.11.2",
    "cute-stack": "1.2.1",
    "grunt": "0.4.5",
    "grunt-bump": "0.3.1",
    "grunt-complexity": "0.3.0",
    "grunt-contrib-jshint": "0.11.2",
    "grunt-deps-ok": "0.8.0",
    "grunt-jsonlint": "1.0.4",
    "grunt-lineending": "0.2.4",
    "grunt-nice-package": "0.9.2",
    "grunt-readme": "0.4.5",
    "jsc": "0.3.11",
    "jshint-stylish": "2.0.1",
    "matchdep": "0.3.0",
    "mocha": "2.2.5",
    "pre-git": "1.2.4",
    "time-grunt": "1.2.1"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "homepage": "https://github.com/bahmutov/gt",
  "keywords": [
    "testing",
    "unit test",
    "unit testing",
    "js",
    "javascript",
    "coverage",
    "code coverage",
    "istanbul",
    "qunit",
    "node"
  ],
  "license": "MIT",
  "main": "./gt.js",
  "preferGlobal": true,
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/gt.git"
  },
  "scripts": {
    "test": "npm run mocha && npm run unit && npm run e2e",
    "unit": "node bin/gt.js ./examples/all.js --colors true --output",
    "e2e": "node bin/gt.js tests/all.js --colors true --output",
    "cover": "istanbul cover gt tests",
    "complexity": "node ./node_modules/jsc gt.js src/assertions/*.js src/utils/*.js src/*.js src/UnitTest/*.js --colors false",
    "doc": "xplain --title gt -v 0.8.* -i src/assertions/*.js",
    "mocha": "node node_modules/mocha/bin/_mocha tests/mocha -R spec",
    "coveralls": "cat cover/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "bdd": "node bin/gt.js examples/bdd/spec.js --bdd",
    "commit": "commit-wizard"
  }
}
